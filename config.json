{
  "PORT": 5999,

  
  "distros": {
    "debian": {
      "type": "debian-based",
      "repoURL": "http://ftp.debian.org/debian/",
      "rootFolder": "dists/",
      "contentsFilter": {
        "basicFilter": "^Contents-.*$",
        "excludeFilter": []
      },
      "components": [
        "main/",
        "contrib/",
        "non-free/"
      ],
      "releaseNamesFilter": {
        "basicFilter": "^[a-z]*/$",
        "excludeFilter": [
          "stable/",
          "experimental/",
          "oldoldstable/",
          "oldstable/",
          "sid/",
          "testing/",
          "unstable/",
          "Parent Directory"
        ]
      },
      "archsFilter": {
        "basicFilter": "^binary-.*/$",
        "excludeFilter": []
      }
    },
    "ubuntu": {
      "type": "ubuntu-based",
      "repoURL": "http://archive.ubuntu.com/ubuntu/",
      "rootFolder": "dists/",
      "contentsFilter": {
        "basicFilter": "^Contents-.*$",
        "excludeFilter": []
      },
      "components": [
        "main/",
        "multiverse/",
        "restricted/",
        "universe/"
      ],
      "releaseNamesFilter": {
        "basicFilter": "^[a-z]*/$",
        "excludeFilter": [
          "devel/"
        ]
      },
      "archsFilter": {
        "basicFilter": "^binary-.*/$",
        "excludeFilter": []
      }
    }
  }
}